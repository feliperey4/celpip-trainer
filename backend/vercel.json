{
  "name": "celpip-trainer-backend",
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "env": {
    "GEMINI_API_KEY": "@gemini-api-key",
    "DEBUG": "false",
    "HOST": "0.0.0.0",
    "PORT": "8000"
  },
  "functions": {
    "main.py": {
      "maxDuration": 60
    }
  }
}